<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- 设置添加别名注解的类所在的包 -->
	<typeAliases>
		<package name="com.neusoft.newcharge.po"/>
		<package name="com.neusoft.newcharge.vo"/>
	</typeAliases>
	
	<!-- 设置映射文件所在的位置 -->
	<mappers>
		<mapper resource="com/neusoft/newcharge/mapper/EmpMapper.xml"/>
	</mappers>
</configuration>